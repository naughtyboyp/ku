
#include<stdio.h>
int main()
{
    int col,i,j,row,m,n;
    int a[1000][1005];
    scanf("%d%d", &n, &m);
    for(i=0; i<n;i++)
    {
        for(j=0; j<m; j++)
        {
            scanf("%d", &a[i][j]);
        }
    }
    row=col=0;
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
            if(a[i][j]>a[row][col])
            {
                row=i;
                col=j;
            }
        }
    }
    printf("%d", a[row][col]);
    return 0;
}
