//本题关键在于 发现就是判断出二进制的位数
#include<stdio.h>
#include<math.h>
int main()
{
    int t = 0;
    scanf("%d", &t);
    while(t--)
    {
        int n = 0;
        scanf("%d", &n);
        int num = 0;
        while(n)
        {
            num++;
            n>>=1;
        }
        printf("%d\n", num-1);
    }
    return 0;
}
