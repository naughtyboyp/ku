#include<stdio.h>
int main()
{
    int n = 0;
    scanf("%d", &n);
    while(n--)
    {
        int m = 0;
        scanf("%d", &m);//分析问题得出结论  一共有 C（m+m,m）种路线
        int k = 0;
        int isum = 1;
        // 先来把公式变形。
        //(m!)/(n!*(m-n)!)=（m*(m-1)*(m-2)*…*(m-n+2)*(m-n+1))/(n!)=((m-n+1)/1)*((m-n+2)/2)*((m-n+3)/3)*…*((m-n+n)/n)=∏((m-n+k)/k)
        for(k=1;k<=m;k++)//组合数公式- 阶乘
        {
            isum*=(2*m-m+k)/k;//先算乘法，避免先算（m-n+k）/k除不尽带来误差
        }
        printf("%d\n", isum);
    }
    return 0;
}
